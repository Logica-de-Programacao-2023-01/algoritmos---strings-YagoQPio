package main

import "fmt"

func main() {
	var str1, str2 string

	fmt.Println("Digite a primeira string:")
	fmt.Scanln(&str1)

	fmt.Println("Digite a segunda string:")
	fmt.Scanln(&str2)

	if str1 == str2 {
		fmt.Println("As strings sÃ£o iguais!")
	} else {
		fmt.Println("As strings sÃ£o diferentes!")
	}
}
